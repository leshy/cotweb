  "main": "dist/index.js",
  "module": "dist/cotwebsocket.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "buildclient": "npx parcel build",
    "buildall": "dts build && npx parcel build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "targets": {
    "frontend": {
      "source": "src/clientside.tsx",
      "distDir": "web/static/js/",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/cotwebsocket.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cotwebsocket.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
