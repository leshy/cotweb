{
  "name": "cotwebsocket",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Ivan Nikolic",
  "main": "dist/index.js",
  "module": "dist/cotwebsocket.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": false
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/cotwebsocket.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/cotwebsocket.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "@tsconfig/recommended": "^1.0.2",
    "@types/engine.io": "^3.1.7",
    "@types/express": "^4.17.17",
    "dts-cli": "^2.0.3",
    "husky": "^8.0.3",
    "parcel": "^2.9.2",
    "size-limit": "^8.2.4",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "engine.io": "^6.4.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "lsh-foundation": "^0.1.4"
  }
}
